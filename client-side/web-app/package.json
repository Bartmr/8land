  "scripts": {
    "postinstall": "node ./polyfill-types-mocha.js",
    "clean:typings": "rimraf dist && find _graphql-generated_ -name \"*.ts\" -type f -delete && find src -name \"*._graphql-generated_.ts\" -type f -delete",
    "clean": "gatsby clean && rimraf dist .webpack-bundle-analyzer _graphql-generated_ && find src -name \"*._graphql-generated_.ts\" -type f -delete",
    "build": "gatsby build --prefix-paths",
    "build:ci": "./shared-libraries.sh && CI=true npm run build && npm run source-maps:release",
    "source-maps:release": "mkdir -p dist/sourcemaps && mv public/*.map dist/sourcemaps && find public -name \"*.map\" -type f -delete",
    "develop": "gatsby develop",
    "serve": "gatsby serve --prefix-paths --host 0.0.0.0",
    "typecheck": "tsc -p tsconfig.typecheck.json",
    "typecheck:declaration-files": "tsc -p tsconfig.declaration-files.json",
    "lint": "eslint --ext .ts,.tsx --max-warnings 0 .",
    "lint:fix": "eslint --ext .ts,.tsx --max-warnings 0 --fix .",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "integrity-check": "npm run clean && IS_INTEGRITY_CHECK=true concurrently \"npm run typecheck:declaration-files\" \"npm run build\" -n \"typecheck:declaration-files,build\" && npm run typecheck && npm run lint",
    "integrity-check:fast": "npm run clean:typings && graphql-codegen --config codegen.yml && npm run typecheck && npm run lint"
  }
